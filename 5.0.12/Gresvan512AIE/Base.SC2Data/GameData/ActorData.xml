    <CActorUnit id="Nexus">
        <PlacementActorModel value="NexusPlacementModel"/>
        <On Terms="ActorCreation" Target="::external.NexusShieldBatteryRangeIndicatorHelperList" Send="Signal NexusAddMe"/>
        <On Terms="ActorDestruction" Target="::external.NexusShieldBatteryRangeIndicatorHelperList" Send="Signal NexusRemoveMe"/>
        <On Terms="Signal.*.DestroyBatteryOverchargeRange" Target="BatteryOverchargeRange" Send="Destroy"/>
    </CActorUnit>
    <CActorModel id="NexusPlacementModel">
        <Aliases value="_Main"/>
        <Aliases value="_Unit"/>
        <Aliases value="_Selectable"/>
        <Filter index="Neutral" value="1"/>
        <Filter index="Enemy" value="1"/>
        <FogVisibility value="Visible"/>
        <On Terms="ActorCreation; ModelIsLowQuality" Send="SetOpacity 0.300000"/>
        <On Terms="ActorCreation" Send="AnimGroupApply Placement"/>
        <On Terms="ActorCreation" Send="$Stand PlayForever"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <On Terms="Signal.*.HidePlacement" Send="SetVisibility"/>
        <On Terms="Signal.*.ShowPlacement" Send="SetVisibility 1"/>
        <On Terms="ActorCreation" Send="Create BatteryOverchargeRange"/>
        <HostSiteOps Ops="SOpTipabilityFloorScope"/>
    </CActorModel>
