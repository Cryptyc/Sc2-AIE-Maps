    <CActorList id="NexusShieldBatteryRangeIndicatorHelperList">
        <On Terms="ActorCreation" Send="ExternalFinderAdd"/>
        <On Terms="Signal.*.NexusAddMe" Send="RefSetFromMsg ::actor.NexusToAdd ::Sender"/>
        <On Terms="Signal.*.NexusAddMe" Send="ListAdd ::actor.NexusToAdd"/>
        <On Terms="Signal.*.NexusAddMe; IsStatus TargetOn 1" Target="::actor.NexusToAdd" Send="Create BatteryOverchargeRange"/>
        <On Terms="Signal.*.NexusRemoveMe" Send="RefSetFromMsg ::actor.NexusToRemove ::Sender"/>
        <On Terms="Signal.*.NexusRemoveMe" Send="ListRemove ::actor.NexusToRemove"/>
        <On Terms="Signal.*.ShieldBatteryPlacementOn" Target="::Contents" Send="Create BatteryOverchargeRange"/>
        <On Terms="Signal.*.ShieldBatteryPlacementOff" Target="::Contents" Send="Signal DestroyBatteryOverchargeRange"/>
        <On Terms="GameInitialize" Send="Create"/>
    </CActorList>
    <CActorRange id="BatteryOverchargeRange" abil="BatteryOvercharge">
        <Flags index="SuppressSaveLoad" value="1"/>
        <Filter index="Ally" value="1"/>
        <Filter index="Neutral" value="1"/>
        <Filter index="Enemy" value="1"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <!--On Terms="Abil.BatteryOvercharge.TargetOn" Send="Create"/>
        <On Terms="Abil.BatteryOvercharge.TargetOff" Send="Destroy"/>
        <On Terms="Abil.BatteryOvercharge.ButtonHoverOn" Send="Create"/>
        <On Terms="Abil.BatteryOvercharge.ButtonHoverOff" Send="Destroy"/-->
        <On Terms="SelectionLocalUpdate.Nexus.Start" Send="Create"/>
        <On Terms="SelectionLocalUpdate.Nexus.Stop" Send="Destroy"/>
        <Icon value="Assets\Textures\RadarIcon2.dds"/>
        <IconArcLength value="1.750000"/>
        <Range value="9.800000"/>
    </CActorRange>
